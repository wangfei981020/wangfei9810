spring.datasource.master.dc.timeBetweenEvictionRunsMillis = 600000
spring.datasource.master.dc.minEvictableIdleTimeMillis = 300000
spring.datasource.master.dc.validationQuery = SELECT 1 FROM DUAL
spring.datasource.master.dc.testWhileIdle = true
